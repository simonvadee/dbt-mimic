{
  "terminal.integrated.profiles.osx": {
    "bash": {
        "path": "zsh",
        "args": ["-c", "if [ -e .env ]; then export `grep -v '#' .env`; fi; zsh;"]
    }
  },
  "terminal.integrated.profiles.linux": {
    "bash": {
        "path": "zsh",
        "args": ["-c", "if [ -e .env ]; then export `grep -v '#' .env`; fi; zsh;"]
    }
  }
}
